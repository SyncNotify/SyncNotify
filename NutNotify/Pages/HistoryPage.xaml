<Page
    x:Class="SyncNotify.Pages.DiaglogPages.HistoryPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SyncNotify.Pages.DiaglogPages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="HistoryPage"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <Grid>
        <TextBlock
            x:Name="History_TextBlock"
            Margin="55,15,0,0"
            FontSize="40"
            FontWeight="Bold"
            Text="历史消息"
            TextWrapping="Wrap" />
        <StackPanel Grid.Row="2">
            <TextBlock
                Margin="55,80,0,0"
                FontSize="15"
                Text="ComingSoon~" />


        </StackPanel>
    </Grid>
</Page>
