<ui:Page
    x:Class="SyncNotify.Pages.DiaglogPages.HistoryPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SyncNotify.Pages.DiaglogPages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.inkore.net/lib/ui/wpf/modern"
    Title="HistoryPage"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <Grid>
        <ScrollViewer>
            <StackPanel>
                <TextBlock
                    x:Name="historyTextBlock"
                    Margin="55,15,0,0"
                    FontSize="40"
                    FontWeight="Bold"
                    Text="历史消息"
                    TextWrapping="Wrap" />
                <!--  主消息面板的border  -->
                <StackPanel x:Name="multiple_message_stack_panel">
                    
                </StackPanel>
                
            </StackPanel>
        </ScrollViewer>
    </Grid>
</ui:Page>
